contract get_fib_n(n:int){
        let last_fib_calculated: int = 1;

        entry get_fib(n: int){
            let fib_n: int = 1;
            let fib_n_1: int = 0;

            while(last_fib_calculated < n){
                let temp:int = fib_n;
                fib_n = fib_n + fib_n_1;
                fib_n_1 = fib_n;
                last_fib_calculated = last_fib_calculated + 1;
            }            
            let result:int = fib_n;
        }

    }