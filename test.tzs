contract get_fib_n(n:int){
        let last_fib_calculated: int = 1 ;

        entry get_fib(n: int){
            let result: int = fib(n);
            last_fib_calculated = result;
        }

        func fib(n: int) : int{
            else {
                let a: nat = 0 - 1;
                let b: int = n - 2;
                return fib(a) + fib(b);
            }
        }
        
    }